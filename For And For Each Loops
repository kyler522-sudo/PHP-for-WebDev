<?php

$family = array("Mohit", "Tushar", "Alankrit", "Kunal");

foreach ($family as $key => $value) {
    
    $family[$key] = $value." Rao";
    
    echo "Array item ".$key." is ".$value."<br>";
    
}



for ($i = 0; $i < sizeof($family); $i++) {
    
    echo $family[$i]."<br>";
    
}



for ($i = 10; $i >= 0; $i--) {
    
    echo $i."<br>";
    
}


?>
